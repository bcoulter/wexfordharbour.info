<?xml version="1.0" encoding="UTF-8" ?>
<widget id="info.wexfordharbour.app" 
    android-versionCode="10014"
    version="1.0.14" >
 xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">

<!-- versionCode is optional and Android only -->

  <name>wexfordharbour.info</name>

  <description>
       Wexford Harbour Information
  </description>

    <author email="brian@wexfordharbour.info" href="http://wexfordharbour.info">
        wexfordharbour.info Team
    </author>
	
  <!-- Allow everything but only from the same origin and foo.com -->
  <meta http-equiv="Content-Security-Policy" content="default-src 'self' foo.com">
  
  <plugin name="cordova-plugin-inappbrowser" source="npm" />
  <plugin name="cordova-plugin-whitelist" source="npm" />
  <access origin="http://wexfordharbour.info/" subdomains="true" />
  <access origin="http://wexfordharbour.info/ajaxapp/" subdomains="true" />
  <access origin="http://wexfordharbour.info/css/" subdomains="true" />

</widget>

